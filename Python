from cs50 import get_int

x = get_int("x: ")
y = get_int("y: ")

print(x + y)

x = input("x: ") #Taking input as string
y = input("y: ")

print(int(x) + int(y))




x = get_int("x: ")
y = get_int("y: ")


if x > y:
    print("x is greater than y")
elif x < y:
    print("y is greater than to x")
else:
    print("both are equal")





s = input("s : ").lower()
# t = s.lower()

if s in ['y', 'yes']:
    print("Agreed", end = '!')
elif s in ['no', 'n']:
    print("Not agreed", end = '!')



s = input("s : ")
i = 0
for i in range(len(s)):
    print(s[i].upper(), end = "")
    i += 1
print()


